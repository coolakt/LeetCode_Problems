class Solution {
public:
    vector<vector<int>> flipAndInvertImage(vector<vector<int>>& img) {
        vector<vector<int>> ans;
        for(int i=img.size()-1; i>=0; i--)
        {
            for(int j=img.size()-1; j>=0; j--)
            {
                
                if(img[i][j])
                img[i][j]=0;
                else
                img[i][j]=1;
                ans.push_back(img[i][j]);
            }
        }
        return ans;
    }
};
